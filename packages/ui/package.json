{
  "name": "@template/ui",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./openapi.json": "./openapi.json",
    "./apiClient": "./src/apiClient/index.ts",
    "./apiClient/*": "./src/apiClient/*",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*.tsx",
    "./components/layout/*": "./src/components/layout/*",
    "./guards": "./src/guards/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./lib": "./src/lib/index.ts",
    "./pages": "./src/pages/index.ts",
    "./store": "./src/store/index.ts",
    "./store/types": "./src/store/types/index.ts",
    "./store/types/*": "./src/store/types/*",
    "./store/*": "./src/store/*",
    "./pages/*": "./src/pages/*.tsx",
    "./lib/auth/token": "./src/lib/auth/token.ts",
    "./test": "./src/test/index.ts",
    "./test/*": "./src/test/*"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate:sdk": "OPENAPI_SPEC_PATH=./openapi.json openapi-ts -f openapiTs.config.ts && bun run generate:msw",
    "generate:msw": "bun scripts/generateMswHandlers.ts"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.17",
    "@hey-api/client-fetch": "^0.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.1",
    "@tanstack/react-query": "^5.90.5",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "msw": "^2.12.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
